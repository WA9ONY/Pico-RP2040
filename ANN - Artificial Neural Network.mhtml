hidden =3D tf.keras.layers.Dense(units=3D<span class=3D"hljs-number">2</spa=
n>, activation=3D<span class=3D"hljs-string">'sigmoid'</span>)(inputs)
outputs =3D tf.keras.layers.Dense(units=3D<span class=3D"hljs-number">1</sp=
an>, activation=3D<span class=3D"hljs-string">'sigmoid'</span>)(hidden)

model =3D tf.keras.Model(inputs=3Dinputs, outputs=3Doutputs)
model.<span class=3D"hljs-built_in">compile</span>(loss=3D<span class=3D"hl=
js-string">'binary_crossentropy'</span>, optimizer=3D<span class=3D"hljs-st=
ring">'adam'</span>, metrics=3D[<span class=3D"hljs-string">'accuracy'</spa=
n>])
